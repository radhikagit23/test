pipeline {
	environment{
		JAVA_HOME = "/usr/lib/jvm/java-1.8.0-openjdk-amd64"
	}
	agent { label 'test' }
	stages
	{
		
		stage('Checkout')
		{
			steps { 
				git changelog: false, credentialsId: 'github', poll: false, url: 'https://github.com/radhikagit23/test.git'
			}
		}
		stage('Build War')
		{
		
			steps { 
				sh "mvn clean package"
			}
		}
        
    }
}

